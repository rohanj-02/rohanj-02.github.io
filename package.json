{
  "homepage": "http://rohanj-02.github.io",
  "name": "rohanj-02.github.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@octokit/core": "^3.1.2",
